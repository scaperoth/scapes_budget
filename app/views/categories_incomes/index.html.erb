<h1>Listing categories_incomes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @categories_incomes.each do |categories_income| %>
      <tr>
        <td><%= link_to 'Show', categories_income %></td>
        <td><%= link_to 'Edit', edit_categories_income_path(categories_income) %></td>
        <td><%= link_to 'Destroy', categories_income, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

