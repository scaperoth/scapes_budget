<%= simple_form_for(@income) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :frequency %>
    <%= f.input :name %>
    <%= f.input :amount %>
    <%= f.input :notes %>
    <%= f.association :budget %>
    <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
